<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PharmaGuideApp.Pages.MainPageViewListTemp"
              BackgroundColor="White" >
                
  <ContentPage.Padding>
    <OnPlatform x:TypeArguments="Thickness"
                iOS="20,40,20,20"
                Android="20,20,20,20"
                WinPhone="20,20,20,20" />
  </ContentPage.Padding>

  <StackLayout Spacing="10" VerticalOptions="FillAndExpand">
    
    <Label Text="BrandId" TextColor="Red"/>
    <Label Text="{Binding BrandId}" TextColor="Black" />
    <Label Text="BrandName" TextColor="Red"/>
    <Label Text="{Binding BrandName}" TextColor="Black"/>
    <Label Text="DrugCategory" TextColor="Red"/>
    <Label Text="{Binding DrugCategory}" TextColor="Black"/>
    <Label Text="ContentTitle" TextColor="Red"/>
    <Label Text="{Binding ContentTitle}" TextColor="Black"/>
  </StackLayout>
</ContentPage>